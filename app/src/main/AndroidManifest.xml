<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.pengllrn.tegm">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.ShowDevActivity">
        </activity>
        <activity
            android:name=".activity.DevDetailActivity"
            android:label="设备信息">
        </activity>
        <activity
            android:name=".activity.SearchDeviceActivity"
            android:label="设备列表">
        </activity>
        <activity android:name=".activity.DamageApllyActivity"
            android:label="设备报废申请">
        </activity>
    </application>

</manifest>